@model CommissionLoadViewModel
<div class="col-md-12 col-sm-12 col-xs-12">
    <br />
    <div class="x_panel no-print">
        <div class="x_title no-print">
            <h2>Commission Reports</h2>
            <div class="clearfix"></div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 no-print">
            <div class="x_panel">
                <div class="">
                    <h4>Data Filters</h4>
                    <hr class="card-hr2">
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        Select Report
                        <select id="select-report" class="form-control form-group" asp-items="@Model.ReportType"></select>
                    </div>
                    <div id="broker" class="col-lg-6 col-md-6 col-sm-6 col-xs-12 filter">
                        Select Broker
                        <select id="select-broker" name="broker" asp-items="@Model.Brokers" class="form-control form-group">
                            <option value="0">All</option>
                        </select>
                    </div>
                    <div id="client" class="col-lg-6 col-md-6 col-sm-6 col-xs-12 filter">
                        Select Client
                        <select id="select-client" name="client" asp-items="@Model.Clients" class="form-control form-group">
                            <option value="0">All</option>
                        </select>
                    </div>
                    <div id="source" class="col-lg-6 col-md-6 col-sm-6 col-xs-12 filter">
                        Select Source
                        <select id="select-source" name="agent" class="form-control form-group">
                            <option value="0" selected>Alliance</option>
                        </select>
                    </div>
                    <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
                        Payment Status
                        <select id="paymentStatus" class="form-control form-group">
                            <option value="All" selected>All</option>
                            <option value="Paid">Paid</option>
                            <option value="Unpaid">Unpaid</option>
                        </select>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 no-print">
            <div class="x_panel">
                <div class="">
                    <h4>Filter By Check-Out Date</h4>
                    <hr class="card-hr2">
                    <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
                        From Check-Out
                        <div class="form-group">
                            <div class="input-group date">
                                <input id="checkOutStart" type="date" class="form-control startQuery" placeholder="MM/DD/YYYY" />
                                <span class="input-group-addon">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12">
                        To Check-Out
                        <div class="form-group">
                            <div class="input-group date">
                                <input id="checkOutEnd" type="date" class="form-control endQuery" placeholder="MM/DD/YYYY" />
                                <span class="input-group-addon">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </span>
                            </div>
                        </div>
                    </div>
                    <!-- Placeholder so boxes line up-->
                    <div class="col-lg-6 col-md-12 col-sm-12 col-xs-12 invisible">
                        A
                        <div class="form-group">
                            <div class="input-group date">
                                <input type="date" class="form-control" placeholder="MM/DD/YYYY" />
                                <span class="input-group-addon">
                                    <span class="glyphicon glyphicon-calendar"></span>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="start-report" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center spacer-60">
        <span class="h1 text-extra-muted">Select Filters Above and Run Report</span><br /><br /><br />
        <button id="run-report" type="button" class="btn btn-travgreen runreport">
            <i class="fa fa-bar-chart icon-white purchase-button-icon"></i>Run Report
        </button>
    </div>
    <div id="update-report" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center hidden">
        <button type="button" class="btn btn-travgreen runreport">
            <i class="fa fa-bar-chart icon-white purchase-button-icon"></i>Update Report
        </button>
    </div>
    <div id="result" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 spacer-10">
    </div>
</div>

<script src="~/js/vendors/papaparse/papaparse.min.js"></script>
<script src="~/js/list.js"></script>
<script src="~/js/reports/report.js"></script>
<script src="~/js/reports/commission.js"></script>
<script>
    COMMISSION.init();
    REPORT.init();
</script>
