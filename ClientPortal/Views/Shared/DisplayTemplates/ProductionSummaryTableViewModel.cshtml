@model ProductionSummaryTableViewModel
    <hr>
    <p class="h3"><span class="capitalFirstLetter">@Model.AccountType</span>: <span class="tableName">@Model.AccountName</span></p>
    <table class="table table-striped table-bordered jambo_table bulk_action">
        <thead>
            <tr>
                <th class="col-lg-2 col-md-2">@Model.ReportGroupName</th>
                <th class="col-lg-2 col-md-2">Internet Price</th>
                <th class="col-lg-1 col-md-1">You Pay Price</th>
                <th class="col-lg-1 col-md-1">Member Savings</th>
                <th class="col-lg-1 col-md-1">Commission Earned</th>
                <th class="col-lg-1 col-md-1">Paid Date</th>
            </tr>
        </thead>
        <tbody>
            @Html.DisplayFor(m => m.Items)
        </tbody>
    </table>
