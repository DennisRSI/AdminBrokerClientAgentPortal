@model ActivationTableViewModel
    <hr>
    <p class="h3"><span class="capitalFirstLetter">@Model.Type</span>: <span class="tableName">@Model.CompanyName</span></p>
    <table id="@Model.Type-@Model.Id" data-type="@Model.Type" data-id="@Model.Id" class="table table-striped table-bordered jambo_table bulk_action">
        <thead>
            <tr>
                <th class="col-md-1">Card #</th>
                <th class="col-md-1">Activation Date</th>
                <th class="col-md-1">Member</th>
                <th class="col-md-1">Denomination</th>
                <th class="col-md-1">Card Type</th>
                <th class="col-md-1">Card Used</th>
                <th class="col-md-1">Web Visits</th>
                <th class="col-md-2">Campaign</th>
                <th class="col-md-1">Status</th>
                <th class="col-md-1">Phone</th>
                <th class="col-md-1">Email</th>
                <th class="col-md-1">First Name</th>
                <th class="col-md-1">Last Name</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
