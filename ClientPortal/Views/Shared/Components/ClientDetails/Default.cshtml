@model Codes.Service.ViewModels.ClientDetailsViewModel
    <div id="confirmation-modal" class="modal fade confirm-deactivate-modal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true"></span>
                    </button>
                    <h4 class="modal-title">Deactivate Campaign</h4>
                </div>
                <div class="modal-body">
                    <p>Enter a reason for deactivation.</p>
                    <div class="form-group">
                        <div class="col-lg-12 col-sm-12 col-xs-12">
                            <input id="reason" type="text" class="form-control" placeholder="Reason for deactivation">
                        </div>
                    </div>
                    <br />
                    <div class="clearfix"></div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    <button id="deactivate" type="button" class="btn btn-primary">Deactivate</button>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>

    <div class="col-md-12 col-sm-12 col-xs-12">
        <br />
        <div class="x_panel">
            <div class="x_title">
                <h2>Client Management</h2>
                <div class="title-right pull-right">
                    <a type="button" href="#" class="btn btn-sm btn-travnow nav-page-id" data-controller="view" data-page="clientedit" data-id="@Model.ClientId">
                        <i class="fa fa-edit icon-white purchase-button-icon"></i>Edit
                    </a>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <vc:client-info account-id="@Model.ApplicationReference"></vc:client-info>
                    <table class="table table-responsive table-bordered table-condensed table-hover">
                        <tablehead class="bold-table-text">
                            <tr>
                                <th class="bg-info"></th>
                                <th class="bg-info">Physical</th>
                                <th class="bg-info">Virtual</th>
                            </tr>
                        </tablehead>
                        <tablebody>
                            <tr>
                                <td>Cards in Campaigns</td>
                                <td>@Html.DisplayFor(m => m.PhysicalInCampaigns)</td>
                                <td>@Html.DisplayFor(m => m.VirtualInCampaigns)</td>
                            </tr>
                        </tablebody>
                    </table>
                </div>
                <!-- Chart Campaign Distribution -->
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 invisible">
                    <div class="x_panel">
                        <div class="x_content">
                            <div class="text-center"><h4>Campaign Distribution</h4></div>
                            <div id="nocampaigns" class="text-center"><em>No campaigns have been created</em></div>
                            <canvas id="chart-campaign" style="height:350px;"></canvas>
                        </div>
                    </div>
                </div>
            </div>
            <div class="x_panel">
                <div class="x_title">
                    <h2>Client Campaigns</h2>
                    <div class="title-right pull-right">
                        <button type="button" class="btn btn-travgreen btn-sm" data-toggle="modal" data-target=".new-campaign-modal-lg">
                            <i class="fa fa-flag icon-white purchase-button-icon"></i>Add Campaign
                        </button>
                        <div class="modal fade new-campaign-modal-lg" tabindex="-1" role="dialog" aria-hidden="true">
                            <div class="modal-dialog modal-lg">
                                <div class="modal-content">
                                    <form id="addCampaignForm">
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">×</span>
                                            </button>
                                            <h3 class="modal-title" id="myModalLabel2"><span class="fa fa-flag left pull-right"></span>Create Campaign </h3>
                                        </div>
                                        <div class="modal-body">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <h4>@Model.CompanyName Campaign Creation</h4><br />
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 form-group">
                                                    <label for="campaignName">Create Campaign Name</label>
                                                    <input type="text" class="form-control" id="campaignName" name="campaignName" tabindex="1" placeholder="Campaign Name" required>
                                                    <br />
                                                    <label for="faceValue">Face Value</label>
                                                    <input type="number" class="form-control" id="faceValue" name="faceValue" tabindex="3" placeholder="Face value of card" required>
                                                    <br />
                                                    <label for="cardQuantity">Select Quantity</label>
                                                    <input type="number" class="form-control" id="cardQuantity" name="cardQuantity" tabindex="5" placeholder="Card Qty" required>
                                                    <br />
                                                    <label for="cardPrefix">Pre-Alpha Character(s)</label>
                                                    <input type="text" class="form-control" id="cardPrefix" name="cardPrefix" maxlength="3" tabindex="7" placeholder="Alpha character to be at the start of each card." required>
                                                    <br />
                                                    <label for="padding">Padding</label>
                                                    <input type="text" class="form-control" id="padding" name="padding" maxlength="2" tabindex="9" value="6" placeholder="Number of placeholder digits" required>
                                                    <br />
                                                </div>
                                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 form-group">
                                                    <label for="campaignType">Select Card Type</label>
                                                    <select class="form-control nested-dropdown" id="campaignType" name="campaignType" tabindex="2" style="margin-bottom: 0">
                                                        <option>Virtual</option>
                                                        <option>Physical</option>
                                                    </select>
                                                    <br />
                                                    <label for="startNumber">Starting Number</label>
                                                    <input type="number" class="form-control" id="startNumber" name="startNumber" tabindex="4" placeholder="First number of the card sequence." required>
                                                    <br />
                                                    <label for="activationsPerCard">Activations Per Card</label>
                                                    <input type="number" class="form-control" id="activationsPerCard" name="activationsPerCard" tabindex="6" value="1" placeholder="Determines how many activations each individual card number can have." required>
                                                    <br />
                                                    <label for="cardSuffix">Post-Alpha Character(s)</label>
                                                    <input type="text" class="form-control" id="cardSuffix" name="cardSuffix" maxlength="3" tabindex="8" placeholder="Alpha character to be at the end of each card.">
                                                    <br />
                                                    <label for="increment">Increment</label>
                                                    <input type="number" class="form-control" id="increment" name="increment" maxlength="3" tabindex="10" value="3" placeholder="Increment card numbers by this amount" required>
                                                    <br />
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-6 col-sm-6 col-xs-12">
                                                    <span>Select Benefits For This Campaign:</span>
                                                    <div class="checkbox">
                                                        <label>
                                                            <input type="checkbox" class="flat" checked="checked" disabled="disabled"> Hotel Benefit
                                                        </label>
                                                    </div>
                                                    <div class="checkbox">
                                                        <label>
                                                            <input id="benefitCondo" value="true" name="benefitCondo" type="checkbox" class="flat"> Condo Benefit
                                                        </label>
                                                    </div>
                                                    <div class="checkbox">
                                                        <label>
                                                            <input id="benefitShopping" value="true" name="benefitShopping" type="checkbox" class="flat"> Shopping Benefit
                                                        </label>
                                                    </div>
                                                    <div class="checkbox">
                                                        <label>
                                                            <input id="benefitDining" value="true" name="benefitDining" type="checkbox" class="flat"> Dining Benefit
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="row">
                                                <div class="col-md-3 col-sm-6 col-xs-12">
                                                    <h5><i class="fa fa-calendar"></i> &nbsp;Campaign Start Date:</h5>
                                                    <fieldset class="form-horizontal">
                                                        <div class="control-group">
                                                            <div class="controls">
                                                                <div class=" form-group text-center">
                                                                    <input id="startDate" name="startDate" type="date" class="form-control has-feedback-left" placeholder="Start Date" required>
                                                                    <span class="disclaimer-text">MM/DD/YYYY</span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                                <div class="col-md-3 col-sm-6 col-xs-12">
                                                    <h5><i class="fa fa-calendar"></i> &nbsp;Campaign End Date:</h5>
                                                    <fieldset class="form-horizontal">
                                                        <div class="control-group">
                                                            <div class="controls">
                                                                <div class=" form-group text-center">
                                                                    <input id="endDate" name="endDate" type="date" class="form-control has-feedback-left" placeholder="End Date">
                                                                    <span class="disclaimer-text">MM/DD/YYYY</span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </fieldset>
                                                </div>
                                            </div><br />
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 form-group">
                                                    <label for="campaignDescription">Describe This Campaign</label>
                                                    <textarea class="form-control" id="campaignDescription" name="campaignDescription" placeholder="Please describe the campaign." rows="6" required></textarea>
                                                    <br />
                                                </div>
                                            </div>
