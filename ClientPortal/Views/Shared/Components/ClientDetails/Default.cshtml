@model Codes.Service.ViewModels.ClientDetailsViewModel
<div class="col-md-12 col-sm-12 col-xs-12">
    <br />
    <div class="x_panel">
        <div class="x_title">
            <h2>Client Management</h2>
            <div class="title-right pull-right">
                <a type="button" href="#" class="btn btn-sm btn-travnow nav-page-id" data-controller="view" data-page="clientedit" data-id="@Model.ClientId">
                    <i class="fa fa-edit icon-white purchase-button-icon"></i>Edit
                </a>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <vc:client-info account-id="@Model.ApplicationReference"></vc:client-info>
                <table class="table table-responsive table-bordered table-condensed table-hover">
                    <tablehead class="bold-table-text">
                        <tr>
                            <th class="bg-info"></th>
                            <th class="bg-info">Physical</th>
                            <th class="bg-info">Virtual</th>
                        </tr>
                    </tablehead>
                    <tablebody>
                        <tr>
                            <td>Total Cards Assigned</td>
                            <td>@Html.DisplayFor(m => m.PhysicalTotal)</td>
                            <td>@Html.DisplayFor(m => m.VirtualTotal)</td>
                        </tr>
                        <tr>
                            <td>Cards in Campaigns</td>
                            <td>@Html.DisplayFor(m => m.PhysicalInCampaigns)</td>
                            <td>@Html.DisplayFor(m => m.VirtualInCampaigns)</td>
                        </tr>
                        <tr>
                            <td>Available Cards</td>
                            <td>@Html.DisplayFor(m => m.PhysicalAvailable)</td>
                            <td>@Html.DisplayFor(m => m.VirtualAvailable)</td>
                        </tr>
                    </tablebody>
                </table>
            </div>
            <!-- Chart Campaign Distribution -->
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <div class="x_panel">
                    <div class="x_content">
                        <div class="text-center"><h4>Campaign Distribution</h4></div>
                        <div id="nocampaigns" class="text-center"><em>No campaigns have been created</em></div>
                        <canvas id="chart-campaign" style="height:350px;"></canvas>
                    </div>
                </div>
            </div>
        </div>
        <div class="x_panel">
            <div class="x_title">
                <h2>Client Campaigns</h2>
                <div class="title-right pull-right">
                    <button type="button" class="btn btn-travgreen btn-sm" data-toggle="modal" data-target=".new-campaign-modal-lg">
                        <i class="fa fa-flag icon-white purchase-button-icon"></i>Add Campaign
                    </button>
                    <div class="modal fade new-campaign-modal-lg" tabindex="-1" role="dialog" aria-hidden="true">
                        <div class="modal-dialog modal-lg">
                            <div class="modal-content">
                                <form id="addCampaignForm">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">×</span>
                                        </button>
                                        <h3 class="modal-title" id="myModalLabel2"><span class="fa fa-flag left pull-right"></span>Create Campaign </h3>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                <h4>@Model.CompanyName Campaign Creation</h4><br />
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 form-group">
                                                <label for="campaignName">Create Campaign Name</label>
                                                <input type="text" class="form-control" id="campaignName" name="campaignName" placeholder="Campaign Name" required>
                                                <br />
                                                <label for="cardQuantity">Select Quantity</label>
                                                <input type="number" class="form-control" id="cardQuantity" name="cardQuantity" placeholder="Card Qty" required>
                                                <br />
                                                <label>Activations Per Card</label>
                                                <input type="number" class="form-control" id="activations" name="activations" placeholder="Determines how many activations each individual card number can have." required>
                                                <br />
                                                <label>Starting Number</label>
                                                <input type="number" class="form-control" id="startingNumber" name="startingNumber" placeholder="Determines the first number of the card sequence." required>
                                            </div>
                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 form-group">
                                                <label>Select Card Type</label>
                                                <select class="form-control nested-dropdown" id="campaignType" name="campaignType" style="margin-bottom: 0">
                                                    <option>Physical</option>
                                                    <option>Virtual</option>
                                                </select>
                                                <br />
                                                <label>Pre-Alpha Character(s)</label>
                                                <input type="text" class="form-control" id="cardPrefix" name="cardPrefix" placeholder="Alpha character to be at the start of each card." required>
                                                <br />
                                                <label>Post-Alpha Character(s)</label>
                                                <input type="text" class="form-control" id="cardSuffix" name="cardSuffix" placeholder="Alpha character to be at the end of each card." required>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6 col-sm-6 col-xs-12">
                                                <span>Select Benefits For This Campaign:</span>
                                                <div class="checkbox">
                                                    <label>
                                                        <input id="benefitHotel" name="benefitHotel" value="true" type="checkbox" class="flat"> Hotel Benefit
                                                    </label>
                                                </div>
                                                <div class="checkbox">
                                                    <label>
                                                        <input id="benefitCondo" name="benefitCondo" value="true" type="checkbox" class="flat"> Condo Benefit
                                                    </label>
                                                </div>
                                                <div class="checkbox">
                                                    <label>
                                                        <input id="benefitShopping" name="benefitShopping" value="true" type="checkbox" class="flat"> Shopping Benefit
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-3 col-sm-6 col-xs-12">
                                                <h5><i class="fa fa-calendar"></i> &nbsp;Expected Start Date:</h5>
                                                <fieldset class="form-horizontal">
                                                    <div class="control-group">
                                                        <div class="controls">
                                                            <div class=" form-group text-center">
                                                                <input id="startDate" name="startDate" type="date" class="form-control has-feedback-left" placeholder="Start Date" required>
                                                                <span class="disclaimer-text">MM/DD/YYYY</span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                            <div class="col-md-3 col-sm-6 col-xs-12">
                                                <h5><i class="fa fa-calendar"></i> &nbsp;Campaign End Date:</h5>
                                                <fieldset class="form-horizontal">
                                                    <div class="control-group">
                                                        <div class="controls">
                                                            <div class=" form-group text-center">
                                                                <input id="endDate" name="endDate" type="date" class="form-control has-feedback-left" placeholder="End Date" required>
                                                                <span class="disclaimer-text">MM/DD/YYYY</span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                            <div class="col-md-3 col-sm-6 col-xs-12">
                                                <h5><i class="fa fa-clock-o"></i> &nbsp;Expected Start Time:</h5>
                                                <fieldset class="form-horizontal">
                                                    <div class="control-group">
                                                        <div class="controls">
                                                            <div class="form-group text-center">
                                                                <input id="startTime" name="startTime" type="time" class="form-control has-feedback-left" placeholder="Start Time" required>
                                                                <span class="disclaimer-text">HH:MM AM/PM</span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                            <div class="col-md-3 col-sm-6 col-xs-12">
                                                <h5><i class="fa fa-clock-o"></i> &nbsp;Campaign End Time:</h5>
                                                <fieldset class="form-horizontal">
                                                    <div class="control-group">
                                                        <div class="controls">
                                                            <div class="form-group text-center">
                                                                <input id="endTime" name="endTime" type="time" class="form-control has-feedback-left" placeholder="End Time" required>
                                                                <span class="disclaimer-text">HH:MM AM/PM</span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </fieldset>
                                            </div>
                                        </div><br />
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 form-group">
                                                <label for="campaignDescription">Describe This Campaign</label>
                                                <textarea class="form-control" id="campaignDescription" name="campaignDescription" placeholder="All campaigns must be approved before going live. Please describe the campaign for the review process." rows="6" required></textarea>
                                                <br />
                                            </div>
                                        </div>
                                        <hr class="card-hr">
