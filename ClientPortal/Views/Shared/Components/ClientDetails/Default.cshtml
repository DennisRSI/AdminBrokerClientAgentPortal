<script type="text/javascript">
    MENU.init();

    var clientId = $('#id-store').data('clientid');
    CAMPAIGN.init(clientId);

    var labels = [];
    var data = [];

    @foreach (var c in Model.Campaigns)
    {
        @:labels.push("@c.CampaignName");
        @:data.push(@c.CardQuantity);
    }

    if (labels.length > 0) {
        $('#nocampaigns').hide();

        labels.push('Unassigned');
        data.push(@Model.PhysicalAvailable + @Model.VirtualAvailable);

        CLIENTDETAILS.init(@Model.ClientId);
        CLIENTDETAILS.loadChart(labels, data);
    }
</script>
